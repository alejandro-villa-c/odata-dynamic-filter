{
    "name": "odata-dynamic-filter",
    "version": "0.1.10",
    "description": "VueJS dynamic filter component with TypeScript support for querying OData endpoints",
    "keywords": [
        "VueJS",
        "TypeScript",
        "OData",
        "Dynamic",
        "Filter",
        "Query",
        "Component"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alejandro-villa-c/odata-dynamic-filter.git"
    },
    "private": false,
    "main": "./dist/odata-dynamic-filter.common.js",
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "*.json",
        "*.js",
        "*.ts"
    ],
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build-bundle": "vue-cli-service build --target lib --name odata-dynamic-filter ./src/components/DynamicFilter/index.ts"
    },
    "dependencies": {
        "core-js": "^3.6.5",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.3",
        "vue-property-decorator": "^8.4.2"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.4.0",
        "@vue/cli-plugin-typescript": "~4.4.0",
        "@vue/cli-service": "~4.4.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "typescript": "~3.9.3",
        "vue-template-compiler": "^2.6.11"
    }
}
